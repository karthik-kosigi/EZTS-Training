    "arr = [3,4,2,1,9,11,3,4,7]\n",
    "l=[]\n",
    "i=0\n",
    "while(i<len(arr)):\n",
    "    for j in range(i+1,len(arr)):\n",
    "        if arr[i]==arr[j]:\n",
    "            l.append(j-i+1) \n",
    "    i+=1\n",
    "print(min(l))"
   ]
