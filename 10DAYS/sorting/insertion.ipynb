   "source": [
    "# insertion sort  O(n^2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 2, 3, 4, 5, 6, 7, 8, 9]\n"
     ]
    }
   ],
   "source": [
    "a=[4,2,1,5,3,9,8,6,7]\n",
    "n=len(a)\n",
    "\n",
    "for i in range(1,n):\n",
    "    key = a[i]\n",
    "    j=i-1\n",
    "    while j>=0 and a[j]>key:\n",
    "        a[j+1]=a[j]\n",
    "        j-=1\n",
    "    a[j+1]=key\n",
    "print(a)"
   ]
  }
 ],
